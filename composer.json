{
    "name": "serendipity_hq/bundle-users",
    "description": "A Symfony bundle that helps manage users in a Symfony app.",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "serendipity_hq/console-styles-bundle": "dev-dev",
        "symfony/console": "^3.4|4.4|^5.2",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "drenso/phan-extensions": "@stable",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phan/phan": "^3.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "@stable",
        "phpstan/phpstan-phpunit": "@stable",
        "phpstan/phpstan-symfony": "@stable",
        "rector/rector": "^0.7",
        "roave/security-advisories": "dev-master",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/test-pack": "^1.0",
        "thecodingmachine/phpstan-safe-rule": "@stable",
        "vimeo/psalm": "^3.12",
        "weirdan/doctrine-psalm-plugin": "@stable"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\UsersBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\UsersBundle\\Tests\\": "tests"
        }
    },
    "conflict": {
        "nikic/php-parser": "4.6.0"
    }
}
